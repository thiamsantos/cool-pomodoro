{
  "name": "cool-pomodoro",
  "version": "1.0.0",
  "description": "Pomodoro timer built with cool libraries",
  "scripts": {
    "test": "tape -r babel-register src/js/**/*.test.js",
    "coverage": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint:styl": "stylint ./src/styl/ -c .stylintrc",
    "lint:js": "xo src/js/**/*.js",
    "lint": "npm run lint:js && npm run lint:styl",
    "build:js": "rollup -c",
    "build:styl": "stylus -c -u poststylus --with \"['autoprefixer', 'css-mqpacker']\" src/styl/main.styl -o dist/css",
    "watch:test": "tape-watch -r babel-register src/js/**/*.test.js",
    "serve": "browser-sync start -s dist --files \"dist/**/*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiamsantos/cool-pomodoro.git"
  },
  "author": "Thiago Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiamsantos/cool-pomodoro/issues"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "new-cap": 0
    },
    "space": true,
    "semicolon": false
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": ["transform-object-rest-spread"],
    "sourceMaps": "inline"
  },
  "homepage": "https://github.com/thiamsantos/cool-pomodoro#readme",
  "devDependencies": {
    "autoprefixer": "^6.6.1",
    "babel-core": "^6.21.0",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "bel": "^4.5.1",
    "browser-sync": "^2.18.6",
    "css-mqpacker": "^5.0.1",
    "morphdom": "^2.3.1",
    "nyc": "^10.0.0",
    "poststylus": "^0.2.3",
    "redux": "^3.6.0",
    "rollup": "^0.41.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^3.2.2",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4",
    "xo": "^0.17.1"
  }
}
